signature:
  & : 2 # conjunction
  | : 2 # disjunction

axioms:
   # properties of meet
   x & x = x;
   x & (y & z) = (x & y) & z;
   # properties of join
   x | x = x;
   x | (y | z) = (x | y) | z;
   # absorption laws
   x & (x | y) = x;
   (x | y) & y = y;
   x | (x & y) = x;
   (x & y) | y = y;

