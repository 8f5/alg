# The axioms for a commutative semiring
signature:
        m : 2 #add
        n : 2 #mult
        e : 0 #additive unit
        f : 0 #multiplicative unit

axioms:
        m(e,x) = x;
        m(x,e) = x;
        m(x,m(y,z)) = m(m(x,y),z);
        m(x,y) = m(y,x);

        n(f,x) = x;
        n(x,f) = x;
        n(x,n(y,z)) = n(n(x,y),z);
        n(x,y) = n(y,x);

        n(m(x,y),z) = m(n(x,z),n(y,z));
        n(x,m(y,z)) = m(n(x,y),n(x,z));
