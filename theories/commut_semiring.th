# The axioms for a commutative unital semiring
signature:
        + : 2
        * : 2
        0 : 0
        1 : 0

axioms:
   0 + x = x;
   x + 0 = x;
   x + (y + z) = (x + y) + z;
   x + y = y + x;
   1 * x = x;
   x * 1 = x;
   x * (y * z) = (x * y) * z;
   x * y = y * x;
   (x + y) * z = x * z + y * z;
   x * (y + z) = x * y + x * z;