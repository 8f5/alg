# The theory of a lattice
signature:
   & : 2 # binary meet
   | : 2 # binary join

axioms:
   # properties of meet
   x & x = x;
   x & y = y & x;
   x & (y & z) = (x & y) & z;
   # properties of join
   x | x = x;
   x | y = y | x;
   x | (y | z) = (x | y) | z;
   # absorption laws
   x & (x | y) = x;
   x | (x & y) = x;

